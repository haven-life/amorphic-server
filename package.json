{
  "name": "amorphic",
  "description": "Front to back isomorphic framework for developing applications with node.js and mongoDB",
  "homepage": "https://github.com/haven-life/amorphic",
  "version": "2.8.1",
  "dependencies": {
    "amorphic-bindster": "2.0.*",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "chalk": "2.4.1",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "formidable": "1.1.1",
    "mongodb-bluebird": "0.1.2",
    "nconf": "0.8.5",
    "persistor": "2.6.*",
    "semotus": "2.4.*",
    "serve-static": "1.13.1",
    "supertype": "3.0.*",
    "uglify-js": "2.7.5"
  },
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/mocha": "2.2.43",
    "@types/node": "8.0.46",
    "axios": "0.17.0",
    "chai": "4.1.2",
    "es6-promise": "4.1.1",
    "eslint": "^5.3.0",
    "istanbul": "0.4.5",
    "knex": "^0.15.2",
    "mocha": "4.0.1",
    "mock-fs": "4.4.1",
    "pg": "^7.4.3",
    "request": "2.87.0",
    "sinon": "4.0.1",
    "ts-node": "3.3.0",
    "typescript": "2.5.3",
    "xmlhttprequest-cookie": "0.9.4"
  },
  "scripts": {
    "compile": "tsc",
    "build": "npm run compile",
    "lint": "eslint .",
    "test": "istanbul cover _mocha -- test/* -R spec",
    "test:docker": "docker-compose up --build",
    "test:trace": "mocha test/*",
    "test:debug": "mocha --debug-brk test/*",
    "test:config": "mocha test/config",
    "test:example": "mocha test/example",
    "test-d": "istanbul cover _mocha -- test/daemon/* -R spec",
    "test-da": "istanbul cover _mocha -- test/daemon_auto/* -R spec",
    "test:ts": "mocha -r ts-node/register typescript_tests/unittest.ts",
    "debug:test:ts": "tsc && mocha --inspect-brk typescript_tests/unittest.js"
  },
  "directories": {},
  "engines": {
    "node": ">=0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/selsamman/amorphic.git"
  },
  "bugs": {
    "url": "http://github.com/selsamman/amorphic/issues"
  },
  "optionalDependencies": {}
}
